
#### go App ####
# go app setup from http://www.github.com/rookie/go
# 1. main command 'go'
function go()
{
    RESULT=$(python ~/bin/go.py $*)
    
    if [[ "$RESULT" == GOCD* ]]; then
        if [ -d "${RESULT:5}" ]; then
            cd ${RESULT:5}
            pwd
        fi
    else
        echo "${RESULT}"
    fi
}
# 2. make sure ~/bin is in the path
export PATH=$PATH:~/bin
