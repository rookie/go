
#go app from http://www.github.com/rookie/go
function go()
{
    RESULT=$(python ~/bin/go.py $*)
    
    if [[ "$RESULT" == GOCD* ]]; then
        if [ -d "${RESULT:5}" ]; then
            cd ${RESULT:5}
            pwd
        fi
    else
        echo "${RESULT}"
    fi
}

export PATH=$PATH:~/bin

